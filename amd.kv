#:kivy 1.0.9
MDBoxLayout : 
    orientation : 'vertical'

    MDScreen :
        MDLabel :
            id : my_label
            text : "tracé géometrique sur des images"
            halign : "center"
        
        #bouton qui liste les different menu
        MDFloatingActionButtonSpeedDial : 
            data : app.data  #le dictionnaire data defini les menu avec leur icone qui sont issue de material design icon 
            root_button_anim: True
            hint_animation: True
            right_pad: False

            #pour lancer des actions quand un bouton est clique
            callback: app.callback



       